package classeGraphique;

import javax.swing.JPanel;
import javax.swing.JLabel;

public class RechercheLivre extends JPanel {

	/**
	 * Create the panel.
	 */
	public RechercheLivre() {
		setLayout(null);
		
		JLabel titre = new JLabel("Paris : 221 établissements trouvés");
		titre.setBounds(53, 59, 265, 35);
		add(titre);
		
		JLabel background_img = new JLabel("");
		background_img.setBounds(0, 0, 818, 423);
		add(background_img);

	}

}
